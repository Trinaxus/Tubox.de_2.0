# Example environment variables for Vite (copy to .env.development and .env.production)
# Access in code via import.meta.env.VITE_*

# Gallery API (PHP)
VITE_SERVER_BASE_URL=https://tubox.de/TUBOX/server/api/gallery-api

# Blog API (PHP)
VITE_BLOG_SERVER_BASE_URL=https://tubox.de/TUBOX/server/api/blog-api

# Base URL for uploaded media
VITE_UPLOADS_BASE_URL=https://tubox.de/TUBOX/server/uploads

# Admin/API token (set a real token for production)
VITE_API_TOKEN=change-me
